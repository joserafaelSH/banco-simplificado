{
  "name": "typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx dotenv-cli -e .env.development ts-node src/server.ts",
    "start:dev": "npx dotenv-cli -e .env.development nodemon src/server.ts",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npx dotenv-cli -e .env.test -- jest --runInBand",
    "test:unit": "npx dotenv-cli -e .env.test -- jest --runInBand --config ./jest.unit.config.ts",
    "test:int": "npx dotenv-cli -e .env.test -- jest --runInBand --config ./jest.int.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "npx dotenv-cli -e .env.test -- jest --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npx dotenv-cli -e .env.test -- jest --runInBand --config ./jest.e2e.config.ts",
    "db:push": "npx dotenv-cli -e .env.development -- npx prisma db push",
    "migrate:dev": "npx dotenv-cli -e .env.development -- npx prisma migrate dev --create-only",
    "migrate:apply": "npx dotenv-cli -e .env.development -- npx prisma migrate dev",
    "prismastudio:dev": "npx dotenv-cli -e .env.development -- npx prisma studio",
    "prismastudio:test": "npx dotenv-cli -e .env.test -- npx prisma studio",
    "jest:file": "npx dotenv-cli -e .env.test npx jest"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "axios": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "pactum": "^3.5.1",
    "prettier": "^3.0.0",
    "prisma": "^5.5.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "src/infra/database/prisma/schema.prisma"
  }
}
